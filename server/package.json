{
  "name": "peoplesoft-tools",
  "description": "An unofficial PeopleSoft plugin for various PeopleSoft related tasks",
  "version": "0.0.1",
  "author": "Microsoft Corporation",
  "license": "MIT",
  "engines": {
    "node": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jatz/PeopleSoftTools-vscode"
  },
  "dependencies": {
    "vscode": "^1.1.5",
    "vscode-languageserver": "^3.4.2"
  },
  "scripts": {
    "installServer": "installServerIntoExtension ../ ./package.json ./tsconfig.json",
    "compile": "installServerIntoExtension ../ ./package.json ./tsconfig.json && tsc -p .",
    "watch": "installServerIntoExtension ../ ./package.json ./tsconfig.json && tsc -w -p ."
  }
}
